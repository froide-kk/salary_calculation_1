<p id="notice"><%= notice %></p>

<h1>Result</h1>


<h2>< contents ></h2>

<h3>Branch : <%= params[:branch]%>, ID : <%= params[:emp_id]%></h3>


<table>
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Age</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @results.each do |result| %>
        <% if result.result_id == 100%>
            <tr>
                <td><%= result.result_id %></td>
                <td><%= result.name %></td>
                <td><%= result.age %></td>

                <td><%= link_to 'Show', result %></td>
                <td><%= link_to 'Edit', edit_result_path(result) %></td>
                <td><%= link_to 'Destroy', result, method: :delete, data: { confirm: 'Are you sure?' } %></td>
            </tr>
        <% end %> 
    <% end %>
  </tbody>
</table>

<br>


