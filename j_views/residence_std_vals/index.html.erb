<p id="notice"><%= notice %></p>

<h1>住居による基準</h1>          <!--Residence Std Vals-->

<table>
  <thead>
    <tr>
      <th>住居地</th>          <!--Residence name-->
      <th>補助金</th>          <!--Residence spt val-->
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @residence_std_vals.each do |residence_std_val| %>
      <tr>
        <td><%= residence_std_val.residence_name %></td>
        <td><%= residence_std_val.residence_spt_val %></td>
        <td><%= link_to '詳しく', residence_std_val %></td>
        <td><%= link_to '修正', edit_residence_std_val_path(residence_std_val) %></td>
        <td><%= link_to '削除', residence_std_val, method: :delete, data: { confirm: '削除しますか？' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to '住居地追加', new_residence_std_val_path %>        <!--New Residence Std Val-->
